    "version": "0.2.0",
    "configurations": [
        {
            "name": "Six Degrees Debugger",
            "type": "cppdbg",
            "request": "launch",
            "program": "/mnt/c/Users/Owner/Documents/Fall_2022_UCSC/CSE_101(Data Structure and Algorithims)/hw_4/Sixdegrees/sixdegrees",
            "args": ["simple-input.txt", "output.txt"],
            "stopAtEntry": true,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }

    ]
}